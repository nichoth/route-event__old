{
  "name": "route-event",
  "version": "4.0.1",
  "description": "Simple client side route event",
  "main": "src/index.js",
  "directories": {
    "example": "example"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./dist": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node ./build.js",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "budo example/index.js:bundle.js --dir example --pushstate --live -- -dv",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "@nichoth/catch-links": "^0.0.4",
    "single-page": "https://github.com/nichoth/single-page/tarball/master"
  },
  "devDependencies": {
    "budo": "^11.8.4",
    "dependency-check": "^3.0.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.23.0",
    "eslint-config-minlint": "^2.0.0",
    "routes": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/route-event.git"
  },
  "keywords": [
    "browser",
    "client-side",
    "client",
    "route",
    "event",
    "router",
    "single",
    "page",
    "location",
    "url",
    "change"
  ],
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/route-event/issues"
  },
  "homepage": "https://github.com/nichoth/route-event#readme"
}
